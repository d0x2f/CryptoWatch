<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="cryptowatch">
  <schema
    id="org.gnome.shell.extensions.cryptowatch"
    path="/org/gnome/shell/extensions/cryptowatch/">

    <key name="portfolio" type="a(sd)">
      <default>[("bitcoin",0.01 ),("ethereum",1)]</default>
    </key>

    <key name="refresh-interval" type="s">
      <default>"0"</default>
    </key>

    <key name="precision" type="i">
      <default>0</default>
    </key>

    <key name="currency" type="s">
      <default>"united-states-dollar"</default>
    </key>

    <key name="coincap-api-key" type="s">
      <default>""</default>
    </key>

  </schema>
</schemalist>
